{
  "expo": {
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#09090B",
        "foregroundImage": "./assets/images/adaptive-icon.png"
      },
      "package": "dev.pedrohb.planner"
    },
    "experiments": {
      "typedRoutes": true
    },
    "icon": "./assets/images/icon.png",
    "ios": {
      "bundleIdentifier": "dev.pedrohb.planner",
      "supportsTablet": true
    },
    "name": "planner",
    "orientation": "portrait",
    "plugins": ["expo-router", "expo-font"],
    "scheme": "planner",
    "slug": "planner",
    "splash": {
      "backgroundColor": "#09090B",
      "image": "./assets/images/splash.png",
      "resizeMode": "contain"
    },
    "userInterfaceStyle": "automatic",
    "version": "1.0.0",
    "web": {
      "bundler": "metro",
      "favicon": "./assets/images/favicon.png",
      "output": "static"
    }
  }
}
